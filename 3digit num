import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int a= s.nextInt();
		if(a>99 && a<1000){
		    System.out.println(a+" is a 3 digit number");
		}
		else{
		     System.out.println(a+" not a 3 digit number");
		
		}
	}
}
