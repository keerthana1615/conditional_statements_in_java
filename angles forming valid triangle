import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int a = s.nextInt();
		int b =s.nextInt();
		int c =s.nextInt();
		if(a+b+c==180 && (a==90 || b==90 ||c==90)){
		    System.out.println("valid triangle");
		}
		else{
		    System.out.println("not a valid triangle");
		}
	}
}
