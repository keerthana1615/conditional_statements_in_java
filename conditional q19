import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner s= new Scanner(System.in);
		int a = s.nextInt();
		int b = s.nextInt();
		int c = a*b;
		if(c<0){
		    System.out.println(c+" is a negative number");
		}
		else{
		    System.out.println(c+" is a positive number");
		}
	}
}
