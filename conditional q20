import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner s= new Scanner(System.in);
		char ch=s.next().charAt(0);
		int ascii =(int) ch;
		if(ascii%2==0){
		    System.out.println(ascii+" is an even number");
		}
		else{
		    System.out.println(ascii+" is an odd number");
		}
	}
}
