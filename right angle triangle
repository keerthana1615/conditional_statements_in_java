import java.util.Scanner;
public class RightAngledTriangleByAngles {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int a = s.nextInt();
        int b = s.nextInt();
        int c = s.nextInt();
        if ((a+ b + c) == 180 && angle1 > 0 && angle2 > 0 && angle3 > 0) {
            if (a==90||b ==90||c==90) {
                System.out.println("The triangle is a right-angled triangle.");
            } else {
                System.out.println("The triangle is a valid triangle but not right-angled.");
            }
        } else {
            System.out.println("The given angles do not form a valid triangle.");
        }
    }
}

