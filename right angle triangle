import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int a = s.nextInt();
		int b=s.nextInt();
		int c=s.nextInt();
		int d=a*a;
		int e=b*b;
		int f =c*c;
		if(d+e==f||d+f==e||f+e==d){
		    System.out.println("right angle triangle");
		}
		else{
		    System.out.println("wrong triangle");
		}
	}
}
