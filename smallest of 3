import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int a= s.nextInt();
		int b= s.nextInt();
		int c= s.nextInt();
		if(a<b && b<c){
		    System.out.println(a+" is smaller");
		}
		else if(b<c)
		{
		     System.out.println(b+" is smaller");
		
		}
		else{
		    System.out.println(c+" is smaller");
		}
	}
}
