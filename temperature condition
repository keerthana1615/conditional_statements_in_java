import java.util.Scanner;

public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int b = sc.nextInt();
		if(b<=16){
		    System.out.println("Cold");
		}
		else if(b>=16 && b<=25){
		    System.out.println("pleasant");
		}
		else if(b>=25 && b<=35){
		    System.out.println("Hot");
		}
		else{
		    System.out.println("Heat wave!!");
		}
	}
}
