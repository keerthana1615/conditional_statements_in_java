import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int a = s.nextInt();
		int b=s.nextInt();
		int c=s.nextInt();
		if(a==b && a==c){
		    System.out.println("equilateral");
		}
		else if(a==c||b==c||a==b){
		    System.out.println("isosceles");
		}
		else{
		    System.out.println("scalene");
		}
	}
}
